h5
  = t "githubIntegration"

if user.hasGithubToken
  h6
    | Intergations completed
  button.is-primary click="revokeGitHub" mpId="mp-github-revoke"
    = t "revokeGithub"
else
  a.button.is-primary target="_blank" href=user.githubRedirectUrl  mpId="mp-github-integrate"
    = t "integrateGithub"
  = yield
